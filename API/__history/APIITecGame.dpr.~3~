program APIITecGame;

uses
  Vcl.Forms,
  uPrincipal in 'uPrincipal.pas' {Form1},
  controlles.Console in 'controllers\controlles.Console.pas',
  DMGlobal in 'DAO\DMGlobal.pas' {DataModule1: TDataModule};

